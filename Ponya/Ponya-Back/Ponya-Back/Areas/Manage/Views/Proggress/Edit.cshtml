@model Proggress
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Manage/Views/Shared/_ManageLayout.cshtml";
}
<div class="container-fluid">
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="Name">Name</label>
                <input asp-for="Name" class="form-control" placeholder="Name">
            </div>
            <div class="form-group col-md-4">
                <label asp-for="Raiting">Raiting</label>
                <input asp-for="Raiting" class="form-control" placeholder="Raiting">
            </div>
           
            
        </div>
        <div class="form-row">

            <div class="form-group col-md-4">
                <label asp-for="IsDeleted">Is Deleted</label>
                <input asp-for="IsDeleted" class="form-control" placeholder="Is Deleted">
            </div>

            
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Edit</button>
        </div>
    </form>
</div>

