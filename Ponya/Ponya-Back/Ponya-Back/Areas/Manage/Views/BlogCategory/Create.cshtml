@model BlogCategory 
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Manage/Views/Shared/_ManageLayout.cshtml";
}

<div class="container-fluid">
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="CatogryName">Catogory Name</label>
                <input asp-for="CatogryName" class="form-control" placeholder=" Catogory Name">
            </div>
            
            
        </div>
        <div class="form-row">

            <div class="form-group col-md-4">
                <label asp-for="IsDeleted">Is Deleted</label>
                <input asp-for="IsDeleted" class="form-control" placeholder="Is Deleted">
            </div>

            
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>
    </form>
</div>


